{
  "language": "javascript",
  "framework": "jest",
  "testCommand": "npm test",
  "testDirectory": "tests",
  "maxRetries": 3,
  "retryDelay": 1000,
  "priorityLevels": {
    "high": 3,
    "medium": 2,
    "low": 1
  },
  "patterns": {
    "failure": ["FAIL", "✕", "Error:", "Expected"],
    "success": ["PASS", "✓"],
    "flaky": ["FLAKY"]
  }
}