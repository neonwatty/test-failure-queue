{
  "name": "calculator-example",
  "version": "1.0.0",
  "description": "A simple calculator with Jest testing to demonstrate Test Failure Queue",
  "main": "src/calculator.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose"
  },
  "keywords": [
    "calculator",
    "testing",
    "jest",
    "tfq",
    "test-failure-queue"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageDirectory": "coverage",
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  }
}