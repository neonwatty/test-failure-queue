{
  "name": "tfq",
  "version": "1.0.2",
  "description": "Multi-language test failure queue manager with multi-language support.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "tfq": "./bin/tfq"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "test": "jest --testPathIgnorePatterns='test-example-fail'",
    "clean": "rm -rf dist",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run clean && npm run lint && npm run test && npm run build",
    "prepare": "npm run build"
  },
  "keywords": [
    "test",
    "queue",
    "sqlite",
    "cli",
    "failure",
    "multi-language"
  ],
  "author": "Jeremy Watt <jermwatt@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neonwatty/tfq.git"
  },
  "bugs": {
    "url": "https://github.com/neonwatty/tfq/issues"
  },
  "homepage": "https://github.com/neonwatty/tfq#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "type": "commonjs",
  "dependencies": {
    "@types/minimatch": "^5.1.2",
    "better-sqlite3": "^12.2.0",
    "chalk": "^5.6.0",
    "commander": "^14.0.0",
    "glob": "^11.0.3",
    "minimatch": "^10.0.3",
    "os-homedir": "^1.0.2"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/glob": "^8.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "jest": "^30.0.5",
    "tree-kill": "^1.2.2",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  }
}
